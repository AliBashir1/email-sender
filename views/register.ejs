    <title>sign up page</title>
</head>


<body>
    <!-- headers -->

    <header class="header group">
        <div class="container">

            <div class="navbar">
                <ul>
                    <li>
                        <a href=""> About </a>
                    </li>

                    <li>
                        <a href=""> Contact Us </a>
                    </li>
                </ul>

            </div>
            
            <div class="header-logo">
                <svg width="80" height="51" viewBox="0 0 210 181" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M208 2L4 99.94L53.9444 110.56L208 2Z" fill="#87E4BA"/>
                    <path d="M208 2L53.9444 110.56V154.22L208 2Z" fill="#87E4BA"/>
                    <path d="M140.684 179L208 2L95.0822 128.26L140.684 179Z" fill="#87E4BA"/>
                    <path d="M95.0822 128.26L208 2L53.9444 154.22L95.0822 128.26Z" fill="#87E4BA"/>
